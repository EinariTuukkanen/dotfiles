{
  "workbench.colorTheme": "RaijÅ«",
  "window.zoomLevel": 0,
  "editor.rulers": [80],
  "editor.multiCursorModifier": "ctrlCmd",
  "python.linting.pylintEnabled": false,
  "python.linting.flake8Enabled": true,
  "extensions.ignoreRecommendations": false,
  "prettier.trailingComma": "es5",
  "prettier.singleQuote": true,
  "prettier.eslintIntegration": true,
  "files.associations": {
    "*.tmpl": "html",
    "config": "properties"
  },
  "html.autoClosingTags": false,
  "window.menuBarVisibility": "toggle",
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  // Set the default
  "editor.formatOnSave": true,
  "editor.fontFamily": "Hack",
  // "editor.fontFamily": "'Hack', 'monospace', monospace, 'Droid Sans Fallback'",
  "debug.console.fontFamily": "Hack",
  "vscode_custom_css.imports": [
    "file:///home/einari/dotfiles/css/vscode-glow-text.css"
  ],
  "vscode_custom_css.policy": true,
  "gitlens.hovers.currentLine.over": "line",
  "gitlens.codeLens.enabled": false,
  // "workbench.colorCustomizations": {
  //   "editor.selectionBackground": "#003049",
  //   "editor.selectionHighlightBackground": "#003049"
  // },
  "breadcrumbs.enabled": true,
  "editor.renderWhitespace": "none",
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.fontLigatures": true,
  "editor.fontSize": 14,
  "workbench.iconTheme": "city-lights-icons-vsc",
  "editor.tabSize": 2,
  "editor.insertSpaces": true,
  "editor.detectIndentation": false,
  "[python]": {
    "editor.tabSize": 4
  }
}
